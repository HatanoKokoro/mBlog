<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<settings>
		<setting name="cacheEnabled" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.blog.web.entity.Note" alias="Note"/>
		<typeAlias type="com.blog.web.entity.Essays" alias="Essays"/>
		<typeAlias type="com.blog.web.entity.SysAbout" alias="SysAbout"/>
		<typeAlias type="com.blog.web.entity.User" alias="User"/>
		<typeAlias type="com.blog.web.entity.Category" alias="Category"/>
		<typeAlias type="com.blog.web.entity.Classification" alias="Classification"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="com/blog/web/mapper/adminNoteMapper.xml"/>
		<mapper resource="com/blog/web/mapper/adminEssaysMapper.xml"/>
		<mapper resource="com/blog/web/mapper/adminAboutMapper.xml"/>
		<mapper resource="com/blog/web/mapper/adminClassificationMapper.xml"/>
		<mapper resource="com/blog/web/mapper/essaysMapper.xml"/>
		<mapper resource="com/blog/web/mapper/noteMapper.xml"/>
		<mapper resource="com/blog/web/mapper/userMapper.xml"/>
		<mapper resource="com/blog/web/mapper/categoryMapper.xml"/>
		<mapper resource="com/blog/web/mapper/classificationMapper.xml"/>
		<mapper resource="com/blog/web/mapper/aboutMapper.xml"/>
	</mappers>
</configuration>